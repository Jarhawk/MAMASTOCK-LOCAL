{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main",
  "description": "Permissions for main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:path:default",
    "core:window:default",
    "core:webview:allow-clear-all-browsing-data",
    "log:default",
    "fs:default",
    "fs:allow-open",
    "fs:allow-read",
    "fs:allow-mkdir",
    "fs:create-app-specific-dirs",
    "fs:allow-appdata-write",
    "fs:allow-appdata-write-recursive"
  ]
}
