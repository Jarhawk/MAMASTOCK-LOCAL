{
    "$schema":  "https://schema.tauri.app/config/2",
    "productName":  "MamaStock Local",
    "identifier":  "com.mamastock.local",
    "version":  "0.1.0",
    "build":  {
                  "devUrl":  "http://localhost:5173",
                  "frontendDist":  "../dist"
              },
    "app":  {
                "windows":  [
                                {
                                    "title":  "MamaStock Local",
                                    "width":  1200,
                                    "height":  800,
                                    "resizable":  true,
                                    "center":  true,
                                    "devtools":  true
                                }
                            ],
                "security":  {
                                 "csp":  null,
                                 "capabilities":  [
                                                      "main"
                                                  ]
                             }
            },
    "bundle":  {
                   "targets":  [
                                   "msi"
                               ],
                   "icon":  [
                                "icons/icon.ico",
                                "icons/32x32.png",
                                "icons/64x64.png",
                                "icons/128x128.png",
                                "icons/128x128@2x.png"
                            ],
                   "windows":  {
                                   "wix":  {
                                               "language":  "en-US"
                                           }
                               }
               },
    "plugins":  {
                    "log":  {
                                "targets":  [
                                                "webview",
                                                "console"
                                            ],
                                "level":  "debug"
                            }
                }
}